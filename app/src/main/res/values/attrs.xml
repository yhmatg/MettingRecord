<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoodleView">
        <attr name="dv_type" format="integer">
            <enum name="path" value="0" />
            <enum name="line" value="1" />
            <enum name="rect" value="2" />
            <enum name="circle" value="3" />
            <enum name="fill_rect" value="4" />
            <enum name="filled_circle" value="5" />
        </attr>

        <attr name="dv_color" format="color" />
        <attr name="dv_stroke" format="dimension|reference" />
    </declare-styleable>
</resources>